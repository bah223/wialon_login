# Telegram Bot settings
BOT_TOKEN=your_telegram_bot_token_here  # Замените на свой токен от @BotFather
TELEGRAM_USER_ID=123456789  # Замените на свой ID (опционально)

# Wialon credentials
WIALON_USERNAME=example  # Имя пользователя для входа в Wialon
WIALON_PASSWORD=example_password  # Пароль для входа в Wialon
WIALON_BASE_URL=https://hosting.wialon.com/login.html  # URL для входа в Wialon
WIALON_API_URL=https://hst-api.wialon.com/wialon/ajax.html  # URL для API запросов

# Tor settings
USE_TOR=false  # Set to true to enable Tor proxy for Wialon requests
TOR_HOST=tor
TOR_PORT=9050

# Database settings
DATABASE_URL=sqlite:///./data/database.db

# Debug settings
DEBUG=false
LOG_LEVEL=DEBUG

# Server settings
HOST=0.0.0.0
PORT=8000
